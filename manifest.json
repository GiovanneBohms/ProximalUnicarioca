{
  "manifest_version": 3,
  "name": "Proximal",
  "description": "proximal",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "icons": {
    "16": "./images/icon16.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "action": {
    "default_icon": "./images/icon48.png"
  }
}
